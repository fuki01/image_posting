<div class="col-5">
<form action="/users" method="get">
<div class="input-group rounded">
    <input type="search" name="search" id="form1" class="form-control" placeholder="検索" aria-label="検索" aria-describedby="search-addon" />
    <button type="submit" class="btn ">
    <i class="fas fa-search"></i>
    </button>
</div>
</form>
</div>
<table class="table table-striped">
<% @users.each do |user|%>
<tr>
  <td>
  <%= render :partial => "layouts/profile_img", :locals=> {user: user} %>
  <%= user.username%>
  </td>
</tr>
<% end %>
</table>